{
    "fiber handler": {
        "prefix": "fiberctx",
        "body": "func ${1:name}(c *fiber.Ctx) error {\n\t${2:content}\n}"
    },
    "echo handler": {
        "prefix": "echoctx",
        "body": "func ${1:}(c echo.Context) error {\n\t${2:content}\n}"
    },
    "test function": {
        "prefix": "ftest",
        "body": "func Test${1:name}(t *testing.T) {\n\t${2:content}\n}"
    }
}
