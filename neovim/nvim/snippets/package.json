{
    "name": "friendly-snippets",
    "engines": {
        "vscode": "^1.22.0"
    },
    "contributes": {
        "snippets": [
            {
                "language": [
                    "json"
                ],
                "path": "./src/json/json.json"
            },
            {
                "language": [
                    "plaintext",
                    "markdown",
                    "tex",
                    "html",
                    "global",
                    "all"
                ],
                "path": "./src/scripting/scripting.json"
            },
            {
                "language": "go",
                "path": "./src/go/go.json"
            },
            {
                "language": "html",
                "path": "./src/html/html.json"
            },
            {
                "language": [
                    "javascript",
                    "typescript"
                ],
                "path": "./src/javascript/jsdoc.json"
            },
            {
                "language": [
                    "javascript",
                    "typescript"
                ],
                "path": "./src/javascript/jest.json"
            },
            {
                "language": [
                    "javascript",
                    "typescript"
                ],
                "path": "./src/javascript/javascript.json"
            }
        ]
    }
}
