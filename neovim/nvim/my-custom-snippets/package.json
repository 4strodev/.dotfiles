{
    "name": "friendly-snippets",
    "engines": {
        "vscode": "^1.22.0"
    },
    "contributes": {
        "snippets": [
            {
                "language": [
                    "plaintext",
                    "markdown",
                    "tex",
                    "html",
                    "global",
                    "all"
                ],
                "path": "./snippets/scripting/scripting.json"
            },
            {
                "language": "go",
                "path": "./snippets/go/go.json"
            },
            {
                "language": "html",
                "path": "./snippets/html/html.json"
            },
            {
                "language": [
                    "javascript",
                    "typescript"
                ],
                "path": "./snippets/javascript/jsdoc.json"
            },
            {
                "language": [
                    "javascript",
                    "typescript"
                ],
                "path": "./snippets/javascript/jest.json"
            }
        ]
    }
}
